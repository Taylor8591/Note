		dvdd-supply = <&cam0_dvdd>;  /* 1.2v */
		reset-gpios = <&gpio1 RK_PD3 GPIO_ACTIVE_LOW>;
		pwdn-gpios = <&gpio1 RK_PD2 GPIO_ACTIVE_HIGH>;
		rockchip,camera-module-index = <0>;
		rockchip,camera-module-facing = "back";
		rockchip,camera-module-name = "YT10120";
		rockchip,camera-module-lens-name = "30IRC-4M-F20";

		port {
			gc08a8_out0: endpoint {
				remote-endpoint = <&dcphy0_in_gc08a8>;
				data-lanes = <1 2>;
			};
		};
	};

	gc2053_0: gc2053-0@37 {
		compatible = "galaxycore,gc2053";
